    "def print_(*args):\n",
    "    print(f\"{args} has a length of {len(args)}.\")\n",
    "    print(args)\n",
    "    print(*args, sep=' ')\n",
    "    for key, value in zip(args[::2], args[1::2]):\n",
    "        print(f\"{key}:{value}\")"
   ]
  },
